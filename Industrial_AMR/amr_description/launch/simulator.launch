<?xml version="1.0"?>
<launch>

  <param name="robot_description" textfile="$(find amr_description)/urdf/amr_description.urdf" />
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name"
        value="/home/ambpl/catkin_ws/src/amr_description/worlds/postoffice.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
</include>
  
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model custom_amr" />
  
  <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher" respawn="false" output="screen"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen"/>

</launch>
